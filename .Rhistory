simulate_member = function(p1,p2,p3,q) {
enroll = rbinom(1,1,p1)
if(!enroll) {
return(0)
}
engage = rbinom(1,1,p2)
if(!engage) {
return(1)
}
n_client_calls = rgeom(1,p3+q)
client_succeeds = rbinom(1,1,p3/(1-p3-q))
if(!client_succeeds) {
return(2)
}
if(client_succeeds) {
return(3)
}
}
